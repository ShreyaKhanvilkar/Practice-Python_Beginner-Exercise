# First
import random
numbers = [random.randint(1,20) for x in range(10)]
first_last = [numbers[0],numbers[-1]]
print(first_last)

# One Line
import random
numbers = [random.randint(1,20) for x in range(10)]
print([numbers[0],numbers[-1]])

# Function
def get_list(numbers):
    return [numbers[0],numbers[-1]
get_list([random.randint(1,20) for x in range(10)])
